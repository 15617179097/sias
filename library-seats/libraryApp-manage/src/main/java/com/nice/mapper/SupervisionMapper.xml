<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nice.mapper.SupervisionMapper" >

    <insert id="insertSupervision" parameterType="com.nice.pojo.Supervision">
         insert into supervision (classroom_id,seats_num,user_id,create_time,state) value (#{classroomId},#{seatsNum},#{userId},NOW(),0)
      </insert>
</mapper>